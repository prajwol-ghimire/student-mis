                      <div>
                        <a href="/viewsusers" class="btn btn-primary btn-block">View All</a>
            <div id="chartContainer" style="height: 300px; width: 100%;"></div>
            <script>
                function getChartData(callback) {

        var Permission_name =[{{#each dashboardinfo}}{{jsonStringify permission_type}},{{/each}}];
        var Permission_Count = [{{#each dashboardinfo}}{{total_count}},{{/each}}];
        var barColors = ["blue", "green", "red"];

        var dashboardInfo = [];
        for (var i = 0; i < Permission_name.length; i++) {
          dashboardInfo.push({ label: Permission_name[i], y: Permission_Count[i], color: barColors[i] });
        }

        callback(dashboardInfo);
      }

      // Construct the CanvasJS chart for the first graph
      window.onload = function () {
        getChartData(function (dashboardInfo) {
          var options = {
            title: {
              text: "Total User Count"              
            },
            data: [              
              {
                type: "column",
                dataPoints: dashboardInfo
              }
            ]
          };
          $("#chartContainer").CanvasJSChart(options);
        });
      };
              </script>



